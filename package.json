{
  "name": "coderschool-jest-workshop",
  "version": "1.0.0",
  "description": "repo for testing workshop with jest and enzyme",
  "main": "index.js",
  "repository": "https://github.com/vivek12345/coderschool-jest-workshop.git",
  "author": "Vivek Nayyar <vivek.of.nayyar@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.5.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/jestSetup.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 10,
        "branches": 10,
        "functions": 10,
        "lines": 100
      }
    }
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
